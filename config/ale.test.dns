$TTL 86400
@ IN SOA debian.test.dns. admin.test.dns. (
    2025101201 ; Serial (YearMonthDayVersion)
    3600       ; Refresh
    1800       ; Retry
    604800     ; Expire
    86400 )    ; Negative Cache TTL
;
@               IN NS debian.test.dns.
debian          IN A 192.168.2.1